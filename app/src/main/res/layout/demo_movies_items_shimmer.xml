<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/view17"
            android:layout_width="110dp"
            android:layout_height="220dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/ic_square_bg"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/ic_square_bg"
            app:layout_constraintStart_toStartOf="@+id/view17"
            app:layout_constraintTop_toTopOf="@+id/view17" />

        <View
            android:id="@+id/view23"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/ic_square_bg"
            app:layout_constraintEnd_toEndOf="@+id/view17"
            app:layout_constraintTop_toTopOf="@+id/view17" />

        <View
            android:id="@+id/view15"
            android:layout_width="24dp"
            android:layout_height="25dp"
            android:layout_marginTop="110dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/ic_circle_round"
            app:layout_constraintEnd_toEndOf="@+id/view17"
            app:layout_constraintTop_toBottomOf="@+id/view23" />

        <View
            android:id="@+id/view26"
            android:layout_width="80dp"
            android:layout_height="10dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/ic_square_bg"
            app:layout_constraintStart_toStartOf="@+id/view17"
            app:layout_constraintTop_toBottomOf="@+id/view19" />

        <View
            android:id="@+id/view36"
            android:layout_width="40dp"
            android:layout_height="10dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/ic_square_bg"
            app:layout_constraintStart_toStartOf="@+id/view17"
            app:layout_constraintTop_toBottomOf="@+id/view26" />

        <View
            android:id="@+id/view19"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="8dp"
            android:background="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="@+id/view17"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/view17"
            app:layout_constraintTop_toBottomOf="@+id/view15" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>